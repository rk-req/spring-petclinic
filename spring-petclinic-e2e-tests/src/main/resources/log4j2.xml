<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF" shutdownHook="disable">
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>

    <appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="E2E - [%t] %d{MM-dd HH:mm:ss} %5p %c{1}:%L - %m%n%exception{full}"/>
        </Console>
    </appenders>
</configuration>
